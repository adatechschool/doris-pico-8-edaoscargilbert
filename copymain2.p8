pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
--lyra's chronicle
--eda, jay et oscar

function _init()
	create_player()
	init_msg()
	--create_msg("lyra", "bonjour", "je suis lyra")
end

function _update()
	if #messages==0 then
			player_movement()
	end
	update_camera()
	update_msg()
	interact(p.x,p.y)
	door_enter()
	door_exit()
end


function _draw()
 cls()
	draw_map()
	draw_player()
	draw_msg()
	
end
-->8
--map

function draw_map()
	map(0,0,0,0,128,64)
end

function check_flag(flag,x,y)
 local sprite=mget(x,y)
 return fget(sprite,flag)
end

function update_camera()
 local camx=flr(p.x/16)*16
 local camy=flr(p.y/16)*16
 camera(camx*8,camy*8)
end
-->8
--player
--offset position de decalage a 0 mais quand deplacement vers la d on decale de 8 px vers la g
function create_player()
 p={
  x=6,y=4,
  ox=0,oy=0,
  start_ox=0,start_oy=0,
  anim_t=0, --1 qd lanim offset start 
  sprite=1,  
  }
end
--on place offset /directiom
function player_movement()
 newx=p.x
 newy=p.y
 if p.anim_t==0 then
	 if (btnp(➡️)) then
	  newx+=1 -- va vers la droite
	  newox=-8 -- 8px vers la gauche
	  newoy=0
	  p.flip=false
	 elseif (btnp(⬅️)) then
	  newx-=1
	  newox=8
	  newoy=0
	  p.flip=true
	 elseif (btnp(⬇️)) then
	  newy+=1
	  newox=0
	  newoy=-8
	 elseif (btnp(⬆️)) then 
	  newy-=1
	  newox=0
	  newoy=8
	 end
 end
 if not check_flag(0,newx,newy)
 and (p.x!=newx or p.y!=newy) then
  p.x=mid(0,newx,127)
  p.y=mid(0,newy,63)
  p.start_ox=newox
  p.start_oy=newoy
  p.anim_t=1
 end
 --animation
 p.anim_t=max(p.anim_t-0.125,0)
 p.ox=p.start_ox*p.anim_t
 p.oy=p.start_oy*p.anim_t
 
 if p.anim_t>=0.5 then
  p.sprite=2
 else
  p.sprite=1
 end
 
 if check_flag(0,newx,newy) then
 	sfx(0)
 end
   
end

function draw_player()
	--spr(p.sprite,p.x*8,p.y*8)
	spr(p.sprite,p.x*8+p.ox,p.y*8+p.oy,1,1,p.flip)
end
-->8
--messages

function init_msg()
	messages={}
end

function create_msg(name,...)
 msg_title=name
 messages={...}
end

function update_msg()
 if btnp(❎) then
  deli(messages,1)
 end
end

function draw_msg()
	if messages[1] then
		local y=100
		if p.y%16>=9 then
			y=10
		end
		--titre
		rectfill(7,y,11+#msg_title*4,y+7,2)
		print(msg_title,10,y+2,9)
		--message
		rectfill(3,y+8,124,y+24,4)
		rect(3,y+8,124,y+24,2)
  print(messages[1],6,y+11,15)
	end
end

function interact(x,y)
	if x==7 and y==50 then
		create_msg("forgeon","bonjour lyra. j'attendais ta venue","le monde va mal")
	end
	if x==7 and y==4 then
		create_msg("test","le test fonctionne", "au revoir")
	end
end


-->8
--doors

function door_enter()
	if check_flag(1,newx,newy) then
	p.x=7
	p.y=62
	end
end

function door_exit()
	if check_flag(2,newx,newy) then
	p.x=7
	p.y=12
	end
end
