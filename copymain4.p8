pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
--lyra's chronicle
--eda, jay et oscar

function _init()
	create_player()
	init_msg()
	--create_msg("narrateur", "bonjour\ntu es lyra", "ton but est de resoudre\nle mal du village","tu vas etre ammene a\ncommuniquer avec les habitants.","ils te fourniront des indices\npour t'aider dans ta quete.")
end

function _update()
	if #messages==0 then
			player_movement()
	end
	update_camera()
	update_msg()
	interact(p.x,p.y)
	door_enter()
	door_exit()
	door_cave_enter()
	door_cave_exit()
end


function _draw()
 cls()
	draw_map()
	draw_player()
	draw_msg()
	
end
-->8
--map

function draw_map()
	map(0,0,0,0,128,64)
end

function check_flag(flag,x,y)
 local sprite=mget(x,y)
 return fget(sprite,flag)
end

function update_camera()
 camx=flr(p.x/16)*16
 camy=flr(p.y/16)*16
 camera(camx*8,camy*8)
end
-->8
--player
--offset position de decalage a 0 mais quand deplacement vers la d on decale de 8 px vers la g
function create_player()
 p={
  x=24,y=58,
  ox=0,oy=0,
  start_ox=0,start_oy=0,
  anim_t=0, --1 qd lanim offset start 
  sprite=1,  
  }
end
--on place offset /directiom
function player_movement()
 newx=p.x
 newy=p.y
 if p.anim_t==0 then
	 if (btn(➡️)) then
	  newx+=1 -- va vers la droite
	  newox=-8 -- 8px vers la gauche
	  newoy=0
	  p.flip=false
	 elseif (btn(⬅️)) then
	  newx-=1
	  newox=8
	  newoy=0
	  p.flip=true
	 elseif (btn(⬇️)) then
	  newy+=1
	  newox=0
	  newoy=-8
	 elseif (btn(⬆️)) then 
	  newy-=1
	  newox=0
	  newoy=8
	 end
 end
 if not check_flag(0,newx,newy)
 and (p.x!=newx or p.y!=newy) then
  p.x=mid(0,newx,127)
  p.y=mid(0,newy,63)
  p.start_ox=newox
  p.start_oy=newoy
  p.anim_t=1
 end
 --animation
 p.anim_t=max(p.anim_t-0.125,0)
 p.ox=p.start_ox*p.anim_t
 p.oy=p.start_oy*p.anim_t
 
 if p.anim_t>=0.5 then
  p.sprite=2
 else
  p.sprite=1
 end  
end

function draw_player()
	--spr(p.sprite,p.x*8,p.y*8)
	spr(p.sprite,p.x*8+p.ox,p.y*8+p.oy,1,1,p.flip)
end
-->8
--messages

function init_msg()
	messages={}
end

function create_msg(name,...)
 msg_title=name
 messages={...}
end

function update_msg()
 if btnp(❎) then
  deli(messages,1)
 end
end

function draw_msg()
	if messages[1] then
		local y=100
		if p.y%16>=9 then
			y=10
		end
		--titre
		rectfill(7+camx*8,y+camy*8,11+#msg_title*4+camx*8,y+7+camy*8,2)
		print(msg_title,10+camx*8,y+2+camy*8,9)
		--message
		rectfill(3+camx*8,y+8+camy*8,124+camx*8,y+24+camy*8,4)
		rect(3+camx*8,y+8+camy*8,124+camx*8,y+24+camy*8,2)
  print(messages[1],6+camx*8,y+11+camy*8,15)
	end
end

function interact(x,y)
	if x==8 and y==57 and not interaction then
		create_msg("forgeron","bonjour lyra.\nj'attendais ta venue.","le monde va mal.\nle moment est venu.","tu es l'elue.","j'ai recu un message de bondol\nl'apothicaire avant sa mort","a l'ouest pres des rochers,","le balbuzard pecheur sera\nton allie.")
		mset(7,2,39)
		interaction = true
	end
	if x==8 and y==2 and not interaction_corbeau then
		mset(7,2,37)
		create_msg("narrateur","le corbeau  laisser tomber\nun message avant de s'envoler","retenez attentivement ce\nmessage","'le vent d'est-ouest\nde force 3 te portera'","une porte s'est ouverte\ndans le desert","a l'est du village")
		interaction_corbeau = true
		mset(43,6,88)
	end
	if x==120 and y==7 and not interaction_parchemin then
		mset(120,6,119)
		create_msg("parchemin","axpk ipb qbkbyob pb\nobmlpb ix irjfbob.","dfpxkq ix lr ix mfboob\nobdxoab ib zfbi")
		interaction_parchemin = true
	end
end


-->8
--doors

function door_enter()
	if check_flag(1,newx,newy) then
	p.x=7
	p.y=62
	end
end

function door_exit()
	if check_flag(2,newx,newy) then
	p.x=23
	p.y=11
	end
end

function door_cave_enter()
	if check_flag(4,newx,newy) then
		p.x=120
		p.y=11
	end
end

function door_cave_exit()
	if check_flag(5,newx,newy) then
		p.x=43
		p.y=7
	end
end
